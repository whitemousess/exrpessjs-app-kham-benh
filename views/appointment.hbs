    </div>

    <div class="container-fluid p-3 mb-2 text-dark my-5  py-5 "
        style=" box-shadow: rgba(60, 64, 67, 0.3) 0px 1px 2px 0px, rgba(60, 64, 67, 0.15) 0px 2px 6px 2px;border-radius:10px">
        <div class="container py-5">
            <div class="row gx-5">
                <div class="col-lg-11" style="margin-left: 55px;">
                    <div class="bg-white text-center  p-5"
                        style=" box-shadow: rgba(60, 64, 67, 0.3) 0px 1px 2px 0px, rgba(60, 64, 67, 0.15) 0px 2px 6px 2px; border-radius:10px">
                        <h1 class="mb-4 text-black">Điền thông tin phiếu khám bệnh</h1>
                        <form class="px-md-2" action="/tai-lieu/lap-phieu-hen" method="post">
                            <div class="row g-2">
                                {{!-- Select doctors --}}
                                <div class="col-12 text-start">
                                     <label class="form-label text-left">Chọn bác sĩ &#8727;</label>
                                    <select class="form-select bg-light" style="border-radius: 10px;" name="Doctor"
                                        required>
                                        {{#each doctors}}
                                        <option value='{"ID":"{{ID}}", "Name":"{{Name}}"}'>{{Name}}</option>
                                        {{/each}}
                                    </select>
                                </div>

                                {{!-- Name input auto --}}
                                <div class="col-12 col-sm-6 text-start">
                                    <label class="form-label text-black" for="name">Họ và tên &#8727;</label>
                                    <input type="text" id="name" class="form-control bg-light" name="Name"
                                        value="{{user.Name}}" required>
                                </div>

                                <input class="d-none" name="Username" type="text" value="{{user.Username}}">

                                {{!-- Phone input auto --}}
                                <div class="col-12 col-sm-6 text-start">
                                    <label class="form-label text-black" for="phone-number">Số điện thoại
                                        &#8727;</label>
                                    <input type="text" id="phone-number" class="form-control bg-light" name="Phone"
                                        value="{{user.Phone}}" required>
                                </div>

                                {{!-- Email input auto --}}
                                <div class="col-sm-12 text-start">
                                    <label class="form-label text-black" for="email">Email</label>
                                    <input type="email" id="email" class="form-control bg-light" name="Email"
                                        value="{{user.Email}}">
                                </div>

                                {{!-- Date of birth input --}}
                                <div class="col-12 col-sm-6 text-start">
                                    <label for="date-of-birth" class="form-label text-black">Ngày sinh &#8727;</label>
                                    <input type="date" class="form-control datetimepicker-input bg-light"
                                        id="date-of-birth" name="DOB" value="{{user.DOB}}" required>
                                </div>

                                {{!-- Sex input --}}
                                <div class="col-12 col-sm-6 text-start">
                                    <label class="form-label text-black" for="sex">Giới tính</label>
                                    <div class="d-flex">
                                        <input style="margin-top: 10px;" class="form-check-input bg-light" type="radio"
                                            name="Gender" id="male" value="Nam" {{#ifEquals user.Gender "Nam"
                                            }}checked{{/ifEquals}}>
                                        <label style="margin-top: 10px; margin-left: 4px;"
                                            class="form-check-label text-black" for="male">Nam</label>
                                        <input style="margin-top: 10px; margin-left: 20px;"
                                            class="form-check-input bg-light" type="radio" name="Gender" id="female"
                                            value="Nữ" {{#ifEquals user.Gender "Nữ" }}checked{{/ifEquals}}>
                                        <label style="margin-top: 10px;" class="form-check-label text-black"
                                            for="female">Nữ</label>
                                    </div>
                                </div>

